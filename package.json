{
  "name": "monimalz",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "ios": "react-native run-ios --scheme 'monimalz-staging' --simulator='iPhone 8'",
    "iosPlus": "react-native run-ios --simulator=\"iPhone 8 Plus\"",
    "android": "react-native run-android --variant=release",
    "androidDebug": "react-native run-android --variant=debug",
    "lint": "eslint ./ --cache --ignore-path .gitignore",
    "start": "NODE_ENV=development node node_modules/react-native/local-cli/cli.js start",
    "start:persist": "PERSIST_STORE=true npm start",
    "start:purge": "PERSIST_STORE=true PURGE_STORE=true npm start",
    "start:network": "NETWORK_DEBUG=true npm start",
    "test": "jest --silent",
    "test:watch": "jest --silent --watch --coverage",
    "test:coverage": "jest --silent --coverage",
    "test:update": "jest --silent --updateSnapshot",
    "prefbsdk": "cd ~/Documents && { mkdir FacebookSDK ; curl -o ~/Documents/FacebookSDK/archive.zip https://origincache.facebook.com/developers/resources/?id=facebook-ios-sdk-current.zip ; cd -; }",
    "fbsdk": "unzip -d ~/Documents/FacebookSDK ~/Documents/FacebookSDK/archive",
    "postfbsdk": "rm -f ~/Documents/FacebookSDK/archive.zip",
    "syncmessages": "node src/utils/generateLang.js",
    "clean-cache": "watchman watch-del-all && rm -rf $TMPDIR/react-* && rm -rf node_modules && yarn",
    "precommit": "./pre-commit.sh",
    "postinstall": "npm run syncmessages && git add src/i18n && git commit -m \"Sync i18n\"",
    "addTesters": "cd ios/ && fastlane addTesters",
    "match": "cd ios/ && fastlane match",
    "beta": "yarn betaIos; cd ../ && yarn betaAndroid",
    "betaIos": "cd ios/ && fastlane beta",
    "betaAndroid": "cd android/ && fastlane beta"
  },
  "dependencies": {
    "base-64": "^0.1.0",
    "base64-js": "^1.2.1",
    "credit-card-type": "^6.0.0",
    "dateformat": "^3.0.2",
    "decko": "^1.2.0",
    "hapi": "^16.5.0",
    "lottie-react-native": "2.2.7",
    "mangopay-cardregistration-js-kit": "^1.2.2",
    "md5": "^2.2.1",
    "ms": "^2.0.0",
    "nes": "^6.4.3",
    "prop-types": "^15.5.10",
    "react": "16.0.0",
    "react-native": "^0.50.3",
    "react-native-awesome-card-io": "^0.7.0",
    "react-native-ble-manager": "^6.2.4",
    "react-native-communications": "^2.2.1",
    "react-native-config": "^0.9.0",
    "react-native-extended-stylesheet": "0.6.0",
    "react-native-fbsdk": "^0.6.3",
    "react-native-fetch-blob": "^0.10.8",
    "react-native-fingerprint-scanner": "^2.2.0",
    "react-native-google-signin": "^0.12.0",
    "react-native-image-picker": "^0.26.6",
    "react-native-image-resizer": "^1.0.0",
    "react-native-invertible-scroll-view": "^1.1.0",
    "react-native-keyboard-spacer": "^0.4.1",
    "react-native-parsed-text": "^0.0.20",
    "react-native-tab-view": "^0.0.70",
    "react-native-typewriter": "^0.5.2",
    "react-native-video": "^2.0.0",
    "react-navigation": "^1.0.0-beta.11",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.1",
    "redux-observable": "^0.17.0",
    "redux-persist": "^4.10.1",
    "reselect": "^3.0.0",
    "rxjs": "^5.4.3",
    "shallowequal": "^1.0.2",
    "styled-components": "^2.2.1",
    "tipsi-stripe": "^4.5.1",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.0",
    "babel-jest": "^21.0.2",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-inline-environment-variables": "^0.2.0",
    "babel-plugin-transform-remove-console": "^6.8.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-native": "^4.0.0",
    "cross-env": "^5.0.5",
    "enzyme": "^3.1.1",
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-import-resolver-react-native": "^0.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^7.3.0",
    "husky": "^0.14.3",
    "jest": "^21.0.2",
    "jest-react-native": "^18.0.0",
    "prettier": "^1.6.1",
    "react-dom": "^16.0.0",
    "react-test-renderer": "^16.0.0",
    "redux-mock-store": "^1.3.0",
    "sync-spreadsheet": "^1.0.7"
  },
  "jest": {
    "preset": "jest-react-native"
  },
  "rnpm": {
    "assets": [
      "./src/assets/fonts"
    ]
  }
}
